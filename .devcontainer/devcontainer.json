{
   "name": "Bun",
   "build": {
      "dockerfile": "Dockerfile",
      "args": {
         "VERSION": "latest"
      }
   },
   "remoteUser": "bun",
   "postStartCommand": "bun install && bun run dev",
   "customizations": {
      "vscode": {
         "extensions": [
            "astro-build.astro-vscode",
            "github.vscode-github-actions",
            "ecmel.vscode-html-css",
            "vivaxy.vscode-conventional-commits"
         ]
      }
   }
}